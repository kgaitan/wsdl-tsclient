          <xs:documentation>Internal tax filed type code as determined by the airline.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ApproximateInd" type="IndType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>When true, this tax breakdown is approximate.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CollectionInd" type="IndType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>When true, this tax breakdown must be collected.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RefundInd" type="IndType" minOccurs="0" maxOccurs="1">
