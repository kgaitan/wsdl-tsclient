      </xs:element>
      <xs:element name="ApproximateInd" type="IndType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>When true, this fee is approximate.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FOID">
    <xs:annotation>
