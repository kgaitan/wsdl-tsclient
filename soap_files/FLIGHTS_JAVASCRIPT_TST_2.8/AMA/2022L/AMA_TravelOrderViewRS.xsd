        <xs:annotation>
          <xs:documentation source="description" xml:lang="en">The amount for this form of payment. Example: 300 USD</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Qualifier" type="IATA_CodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="description" xml:lang="en">Used to specify if this is the old, new or orginal form of payment.

Encoding Scheme: IATA  PADIS code list [9988] Data Indicator</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="Voucher" minOccurs="0">
