        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:any namespace="" maxOccurs="unbounded" />
          </xs:sequence>
          <xs:attribute name="refs" type="InstanceClassRefSimpleType" use="optional">
            <xs:annotation>
              <xs:documentation source="description" xml:lang="en">Object association(s) between this object and any object instance(s).</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="PolicyLink" type="xs:anyURI">
    <xs:annotation>
      <xs:documentation source="description" xml:lang="en">Policy Location Link. Example: www.anyairline.com/thispolicy</xs:documentation>
    </xs:annotation>
  </xs:element>
