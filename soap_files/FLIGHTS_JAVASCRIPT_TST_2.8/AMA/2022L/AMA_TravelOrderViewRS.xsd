    <xs:sequence>
      <xs:element name="ServiceBundle" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <!--Replaced incorrect type "xs:positiveInteger"-->
            <xs:element name="ItemCount" type="xs:long">
              <xs:annotation>
                <xs:documentation source="description" xml:lang="en">Individual service items in bundle count. Example: 4</xs:documentation>
              </xs:annotation>
