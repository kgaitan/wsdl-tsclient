                </xs:complexType>
              </xs:element>
            </xs:choice>
          </xs:sequence>
          <xs:attribute name="ServiceID" type="xs:token" use="required">
            <xs:annotation>
              <xs:documentation>ID for the Service unique across the entire XML document.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="FareDetail" type="FareDetailType" minOccurs="0" maxOccurs="unbounded">
