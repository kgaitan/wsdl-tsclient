                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                              <xs:attributeGroup ref="ObjAssociationAttrGroup" />
                            </xs:complexType>
                          </xs:element>
                          <xs:element ref="CountryCode" minOccurs="0" maxOccurs="unbounded" />
                          <xs:element name="ProvinceCode" type="ProperNameSimpleType" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                              <xs:documentation source="description" xml:lang="en">Exempt province(s).</xs:documentation>
