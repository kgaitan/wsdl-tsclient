  <xs:element name="PhoneContact" type="PhoneContactType">
    <xs:annotation>
      <xs:documentation source="description" xml:lang="en">Phone Contact.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="PhoneNumber" type="V172_PhoneType">
    <xs:annotation>
      <xs:documentation source="description" xml:lang="en">Provides telephone information, and allows to specify telephone number application (ie Home, Business, etc.). May be used for a simle, unstrucutured number, or, in combination with child attributes allows to create a structured telephone number (ie Country Code number, Area Code numbers, etc.)</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="PhoneContactType">
    <xs:annotation>
      <xs:documentation source="description" xml:lang="en">PHONE CONTACT definition.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="V172_PhoneType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="V172_PhoneType">
    <xs:annotation>
      <xs:documentation source="description" xml:lang="en">PHONE NUMBER helper object.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Application" type="ContactAppSimpleType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="description" xml:lang="en">Phone number application. Examples: Billing, Business, Home, Mobile</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Number" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation source="description" xml:lang="en">Phone number text.
Examples:
+01 999-999-9999 ext 1234, 
617-9976

Note: This may be a simple, un-structured phone number, such as +01 999-999-9999 ext 1234 or combined with child attributes to create a structured phone number.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:string">
              <xs:attributeGroup ref="ObjAssociationAttrGroup" />
              <xs:attribute name="CountryCode" type="CodesetValueSimpleType" use="optional">
                <xs:annotation>
