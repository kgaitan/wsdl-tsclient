  <xs:complexType name="ServiceAssociationsType">
    <xs:annotation>
      <xs:documentation>References to the details of this Service.  May include either Passenger Journeys, a Service Definition, or a Selected Seat.</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="SelectedSeat" type="SeatonLegType">
