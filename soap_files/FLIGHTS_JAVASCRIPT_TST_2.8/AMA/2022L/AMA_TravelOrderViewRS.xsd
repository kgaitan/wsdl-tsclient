      <xs:documentation source="description" xml:lang="en">A data type for Affinity Query Country Flight Departure (Origin) information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Departure" type="CountryQueryType">
        <xs:annotation>
          <xs:documentation source="description" xml:lang="en">Country Departure (Origin)  information.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AffinityCountryArriveType">
