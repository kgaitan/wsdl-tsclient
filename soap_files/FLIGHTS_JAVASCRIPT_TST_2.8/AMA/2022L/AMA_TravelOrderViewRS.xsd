      <xs:documentation xml:lang="en">MCO Payment Method definition.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="TicketNumber" type="ContextSimpleType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="description" xml:lang="en">The ticket number of the miscellaneous charge order (MCO).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attributeGroup ref="ObjAssociationAttrGroup" />
