      <xs:documentation source="description" xml:lang="en">A data type for Service Combination Rule.

Examples: CombinationOK, DoNotCombine, Other</xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="ApplicationSimpleType SrvcCombineRuleListType" />
  </xs:simpleType>
  <xs:simpleType name="SrvceCouponListType">
