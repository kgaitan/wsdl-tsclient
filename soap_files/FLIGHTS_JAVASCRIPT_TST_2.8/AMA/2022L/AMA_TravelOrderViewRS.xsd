                              <xs:documentation source="description" xml:lang="en">Seat row number. Example: 2</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="Column" type="xs:string">
                            <xs:annotation>
                              <xs:documentation source="description" xml:lang="en">Seat column to identify a particular seat position on an aircraft</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="SegmentRef">
                      <xs:annotation>
                        <xs:documentation>Reference to the Segment on which this Passenger's Seat has been assigned.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="ServiceDefinitionRef">
                      <xs:annotation>
                        <xs:documentation>Reference to the ServiceDefinition relating to the Seat assigned to the Passenger.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
