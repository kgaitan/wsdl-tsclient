          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="BankID" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="description" xml:lang="en">Routing Number. Example: 0260-0959-3

Note: This is the code assigned by authorities to financial institutions; sometimes called bank routing number.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="ContextSimpleType">
              <xs:attributeGroup ref="ObjAssociationAttrGroup" />
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="AccountType" type="ContextSimpleType" minOccurs="0">
