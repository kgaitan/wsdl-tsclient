        <xs:documentation source="description" xml:lang="en">When true, checks are accepted.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:element name="Cash">
    <xs:annotation>
      <xs:documentation xml:lang="en">Used to indicate payment in cash.</xs:documentation>
    </xs:annotation>
