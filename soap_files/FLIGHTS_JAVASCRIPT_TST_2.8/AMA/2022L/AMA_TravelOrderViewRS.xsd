      <xs:element ref="LoyaltyRedemption" minOccurs="0" />
      <xs:element ref="DirectBill" minOccurs="0" />
      <xs:element ref="Cash" minOccurs="0" />
      <xs:element ref="BankAccount" minOccurs="0" />
      <xs:element ref="Associations" minOccurs="0" />
