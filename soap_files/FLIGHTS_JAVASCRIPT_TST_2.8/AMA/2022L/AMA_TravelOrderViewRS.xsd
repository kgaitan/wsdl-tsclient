                  </xs:annotation>
                </xs:attribute>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="MeasurementSystem" type="MeasurementTypeListType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="description" xml:lang="en">Examples: Imperial, Metric</xs:documentation>
        </xs:annotation>
