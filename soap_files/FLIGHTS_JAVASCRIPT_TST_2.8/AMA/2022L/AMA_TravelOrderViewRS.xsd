  <xs:complexType name="AddressMetadataType">
    <xs:annotation>
      <xs:documentation source="description" xml:lang="en">A data type for ADDRESS Metadata.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="MetadataObjectBaseType">
        <xs:sequence>
          <xs:element ref="Position" minOccurs="0" />
          <xs:element name="AddressType" type="xs:string" minOccurs="0">
            <xs:annotation>
              <xs:documentation>type of address eg Billing, Business, Home</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AddressFields" minOccurs="0">
            <xs:annotation>
              <xs:documentation>list of address fields required by vendor</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="FieldName" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:simpleContent>
                      <xs:extension base="xs:string">
                        <xs:attribute name="Mandatory" type="xs:boolean" use="required">
                          <xs:annotation>
                            <xs:documentation>If this field is required then true, otherwise false.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:extension>
                    </xs:simpleContent>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
