  <xs:complexType name="CodesetMetadataType">
    <xs:annotation>
      <xs:documentation source="description" xml:lang="en">A data type for PADIS/ CODESET Metadata.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="MetadataObjectBaseType">
        <xs:sequence>
          <xs:element name="Source" minOccurs="0">
            <xs:annotation>
              <xs:documentation source="description" xml:lang="en">Codeset Source.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="OwnerID" minOccurs="0">
                  <xs:annotation>
                    <xs:documentation source="description" xml:lang="en">Owner Entity ID. Example: BA</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:simpleContent>
                      <xs:extension base="xs:string">
                        <xs:attribute name="Name" use="optional">
                          <xs:annotation>
                            <xs:documentation source="description" xml:lang="en">Owner Name. Example: British Airways</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="ProperNameSimpleType" />
                          </xs:simpleType>
                        </xs:attribute>
                      </xs:extension>
                    </xs:simpleContent>
                  </xs:complexType>
                </xs:element>
                <xs:element name="File" type="ShortDescSimpleType" minOccurs="0">
                  <xs:annotation>
                    <xs:documentation source="description" xml:lang="en">Codeset file or resource name. Example: PADIS Codeset</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="Version" type="CodesetValueSimpleType" minOccurs="0">
                  <xs:annotation>
                    <xs:documentation source="description" xml:lang="en">Codeset version. Example: 14</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="OtherLanguage" minOccurs="0">
            <xs:annotation>
              <xs:documentation source="description" xml:lang="en">Other (alternate) Language Encoding and Code Description.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element ref="LanguageCode" />
                <xs:element name="Description" type="DescriptionSimpleType" minOccurs="0">
                  <xs:annotation>
                    <xs:documentation source="description" xml:lang="en">Codeset table or list name.  Example: Data Element 9972-Originator Type</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
