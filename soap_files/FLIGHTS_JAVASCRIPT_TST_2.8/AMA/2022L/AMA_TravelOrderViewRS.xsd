              <xs:annotation>
                <xs:documentation source="description" xml:lang="en">Associated Services.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:choice>
                  <xs:element ref="ServiceReference" maxOccurs="unbounded" />
                  <xs:element ref="ServiceID" maxOccurs="unbounded" />
                </xs:choice>
              </xs:complexType>
            </xs:element>
            <xs:element name="Selection" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="description" xml:lang="en">Minimum and Maximum Bundle Item Selection rules.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:choice maxOccurs="2">
                  <!--Replaced incorrect type "xs:positiveInteger"-->
                  <xs:element name="MinimumQuantity" type="xs:long">
                    <xs:annotation>
                      <xs:documentation source="description" xml:lang="en">MINIMUM quantity of bundle items that must be selected.  Example: 2</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <!--Replaced incorrect type "xs:positiveInteger"-->
                  <xs:element name="MaximumQuantity" type="xs:long">
                    <xs:annotation>
                      <xs:documentation source="description" xml:lang="en">MAXIMUM quantity of bundle items that may be selected.  Example: 4</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:choice>
              </xs:complexType>
            </xs:element>
            <xs:element name="BundleID" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="description" xml:lang="en">Unique Bundle ID. Example: BNDL-001</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="UniqueStringID_SimpleType">
                    <xs:attributeGroup ref="ObjectKeyAttrGroup" />
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="Price" type="ServicePriceType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Price for Bundled service</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="BundleName" type="xs:string" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Name of Bundle</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
          <xs:attributeGroup ref="DataListObjAttrGroup" />
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
