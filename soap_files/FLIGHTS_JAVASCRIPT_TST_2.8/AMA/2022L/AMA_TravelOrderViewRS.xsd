        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="FiledInCurrency" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation source="description" xml:lang="en">This group represents fares filed in a currency different from the currency of a point of sale. The content elements represent the original currency and amount as filed.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element ref="CurrCode" />
                </xs:sequence>
                <xs:attribute name="Amount" type="NumericStringLength1to19">
