      <xs:element name="ServiceDefinition" type="ServiceDefinitionType" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ServiceDefinitionRefType">
    <xs:sequence>
      <xs:element name="ServiceDefinitionRefID" type="ID_Type" minOccurs="1" maxOccurs="1">
