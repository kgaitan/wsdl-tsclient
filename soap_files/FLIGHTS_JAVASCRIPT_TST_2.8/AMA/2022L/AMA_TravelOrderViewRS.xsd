          <xs:documentation source="description" xml:lang="en">Email address application. Examples: Personal, Business, SMS</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Address">
        <xs:annotation>
          <xs:documentation source="description" xml:lang="en">The traveler email address for contact.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="EmailID_Type" />
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attributeGroup ref="ObjAssociationAttrGroup" />
  </xs:complexType>
  <xs:element name="EmailContact" type="EmailType">
    <xs:annotation>
      <xs:documentation source="description" xml:lang="en">Email address details, including application (I.e. home, business, etc.).</xs:documentation>
    </xs:annotation>
