      <xs:documentation source="description" xml:lang="en">Email Address representation.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attributeGroup ref="ObjectPolicyMetaAttrGroup" />
      </xs:extension>
    </xs:simpleContent>
