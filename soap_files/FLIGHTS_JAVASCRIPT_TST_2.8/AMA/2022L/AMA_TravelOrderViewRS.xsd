  <xs:complexType name="RuleMetadataType">
    <xs:annotation>
      <xs:documentation source="description" xml:lang="en">A data type for RULE Metadata.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="MetadataObjectBaseType">
        <xs:sequence>
          <xs:element name="RuleID">
            <xs:annotation>
              <xs:documentation source="description" xml:lang="en">Unique Rule ID which was applied during transaction processing. Example: PRICERULE001 Note: Optional rule context and name may also be specified.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:simpleContent>
                <xs:extension base="xs:string" />
              </xs:simpleContent>
            </xs:complexType>
          </xs:element>
          <xs:element name="Name" type="ProperNameSimpleType" minOccurs="0">
            <xs:annotation>
              <xs:documentation source="description" xml:lang="en">Rule name.  Example: Rule001</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Status" minOccurs="0">
            <xs:annotation>
              <xs:documentation source="description" xml:lang="en">Rule status.  Examples: ActionRequired, RuleExecuted, Other</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:simpleContent>
                <xs:extension base="RuleStatusSimpleType" />
              </xs:simpleContent>
            </xs:complexType>
          </xs:element>
          <xs:element name="Values" minOccurs="0">
            <xs:annotation>
              <xs:documentation source="description" xml:lang="en">Rule Value(s).</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="Value" maxOccurs="unbounded">
                  <xs:annotation>
                    <xs:documentation source="description" xml:lang="en">Rule value detail.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element ref="NodePath" minOccurs="0" maxOccurs="unbounded" />
                      <xs:element name="Instruction">
                        <xs:annotation>
                          <xs:documentation source="description" xml:lang="en">Business rule instruction.
Example: [NodePath.r1] {EQ('FR')} [NodePath.r2]{EQ(true)}</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:simpleContent>
                            <xs:extension base="xs:string" />
                          </xs:simpleContent>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element ref="Remarks" minOccurs="0" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
