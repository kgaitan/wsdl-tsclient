        <xs:complexType>
          <xs:sequence>
            <xs:element name="BankID" type="ProperNameSimpleType">
              <xs:annotation>
                <xs:documentation source="description" xml:lang="en">Issuer Name. Example: Bank of America

Note: This is the name of bank or organization issuing the card (e.g., alumni association, bank, fraternal organization, etc.).</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="CardholderAddress" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="description" xml:lang="en">Cardholder Address.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element ref="SimpleAddress" />
            <xs:element ref="StructuredAddress" />
            <xs:element ref="PaymentAddress" />
          </xs:choice>
          <xs:attributeGroup ref="ObjAssociationAttrGroup" />
        </xs:complexType>
      </xs:element>
      <xs:element name="EffectiveExpireDate" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="description" xml:lang="en">Payment Card Effective and/ or Expiration date(s).</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Effective" type="PayCardDateSimpleType" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="description" xml:lang="en">Payment Card Effective Date. Example: 0116</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Expiration" type="PayCardDateSimpleType" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="description" xml:lang="en">Payment Card Expiration Date. Example: 0119</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
          <xs:attributeGroup ref="ObjAssociationAttrGroup" />
        </xs:complexType>
      </xs:element>
      <xs:element name="ApprovalType" type="IATA_CodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation source="description" xml:lang="en">Approval Code Method. Example: M (Manual)

Note: This is used to specify how the approval code for a credit card sale was obtained.

Encoding Scheme: IATA PADIS Code List for data element 9890.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attributeGroup ref="ObjAssociationAttrGroup" />
  </xs:complexType>
