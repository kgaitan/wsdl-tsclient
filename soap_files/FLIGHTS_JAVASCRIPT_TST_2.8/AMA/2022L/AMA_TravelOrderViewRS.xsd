  <xs:element name="DefaultPolicy" type="PolicyDefaultType">
    <xs:annotation>
      <xs:documentation source="description" xml:lang="en">Pre-defined Message Policy information.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="Policies">
    <xs:annotation>
      <xs:documentation source="description" xml:lang="en">Policy information.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Policy" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation source="description" xml:lang="en">Policy detail.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:complexContent>
              <xs:extension base="PolicyType">
                <xs:sequence>
                  <xs:element ref="DefaultPolicy" />
                  <xs:element ref="PolicyAugmentation" />
                </xs:sequence>
              </xs:extension>
            </xs:complexContent>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="PolicyAugmentation" type="PolicyAugmentationType">
    <xs:annotation>
      <xs:documentation source="description" xml:lang="en">Custom Policy Information augmentation point.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="PolicyDefaultType">
    <xs:annotation>
      <xs:documentation source="description" xml:lang="en">A data type for Policy Notification and Text, e.g. for sensitive data notification and/or handling.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Policy" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation source="description" xml:lang="en">Policy Detail.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="DataPolicyInd" type="xs:boolean" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="description" xml:lang="en">When true, data handling policy(s) apply.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="PolicyID" type="CodesetValueSimpleType" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="description" xml:lang="en">Policy Code or ID.  Example: DATA1</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Type" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Policy Type. Examples: Privacy_PII, Payment_PCI, Credentialing</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:choice>
              <xs:element name="Description" type="ShortDescSimpleType">
                <xs:annotation>
                  <xs:documentation source="description" xml:lang="en">Policy short description.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element ref="PolicyLink" />
            </xs:choice>
            <xs:element ref="NodePath" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="description" xml:lang="en">Message Property Path information for this policy-associated element.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="refs" type="InstanceClassRefSimpleType" use="optional">
            <xs:annotation>
              <xs:documentation source="description" xml:lang="en">Object association(s) between this object and any object instance(s).</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PolicyAugmentationType">
    <xs:annotation>
      <xs:documentation source="description" xml:lang="en">A data type for Augmented Message Policy information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Policy" maxOccurs="unbounded">
