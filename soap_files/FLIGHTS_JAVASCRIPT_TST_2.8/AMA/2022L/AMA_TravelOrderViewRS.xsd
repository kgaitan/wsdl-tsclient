        <xs:annotation>
          <xs:documentation>Payment Method detail.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Qualifier" type="IATA_CodeType" minOccurs="0">
              <xs:annotation>
                <xs:documentation source="description" xml:lang="en">Used to specify if this is the old, new, or orginal form of payment.

Encoding Scheme: IATA  PADIS code list 9988 - Data Indicator</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="VoucherMethod" minOccurs="0" />
            <xs:element ref="RedemptionMethod" minOccurs="0" />
            <xs:element ref="PaymentCardMethod" minOccurs="0" />
            <xs:element ref="OtherMethod" minOccurs="0" />
            <xs:element ref="MiscChargeMethod" minOccurs="0" />
            <xs:element ref="DirectBillMethod" minOccurs="0" />
            <xs:element ref="CashMethod" minOccurs="0" />
            <xs:element ref="BankAccountMethod" minOccurs="0" />
            <xs:element ref="Check" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Check (Cheque) Payment</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Promotions" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Applied Promotions for this payment form.

Note: If specified, this individual payment form information will override any globally specified promotion information.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="Promotion" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>Promotion Detail.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:complexContent>
                        <xs:extension base="V172_PromotionType">
                          <xs:sequence>
                            <xs:element ref="Associations">
                              <xs:annotation>
                                <xs:documentation>Association(s), including Passenger and Group.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                          </xs:sequence>
                        </xs:extension>
                      </xs:complexContent>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attributeGroup ref="ObjAssociationAttrGroup" />
        </xs:complexType>
      </xs:element>
    </xs:sequence>
