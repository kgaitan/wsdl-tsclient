          <xs:documentation>Total of all applicable tax amounts included in the price.  If tax breakdowns are provided, this amount should match the sum of the tax breakdown amounts.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AllRefundableInd" type="IndType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>When true, all tax breakdown amounts are refundable.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ApproximateInd" type="IndType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>When true, this tax is approximate.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CollectionInd" type="IndType" minOccurs="0" maxOccurs="1">
