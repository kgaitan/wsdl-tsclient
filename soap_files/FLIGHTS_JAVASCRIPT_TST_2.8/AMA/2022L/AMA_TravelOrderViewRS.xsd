  </xs:element>
  <xs:complexType name="BankAccountMethodType">
    <xs:annotation>
      <xs:documentation xml:lang="en">BANK ACCOUNT Payment Method definition.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Name" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Account Name. Example: JOHN SMITH

Note: This is the name the bank account is held under.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="ProperNameSimpleType">
              <xs:attributeGroup ref="ObjAssociationAttrGroup" />
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="CheckNumber" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Check or draft number. Example: 1234</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attributeGroup ref="ObjAssociationAttrGroup" />
  </xs:complexType>
  <xs:element name="CashMethod" type="CashMethodType">
    <xs:annotation>
      <xs:documentation>Cash payment.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="CashMethodType">
    <xs:annotation>
      <xs:documentation xml:lang="en">CASH Payment Method definition.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Amount" type="CurrencyAmountOptType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Cash Amount. Example: 67.95</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TerminalID" type="ContextSimpleType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Terminal ID where cash was collected. Example: QP776611</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ReceiptID" type="ContextSimpleType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Cash receipt ID. Example: 121717H47S</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element ref="AgentUserID" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Agent ID that collected the cash. Example: TOM123</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Address" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Location where cash was collected.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element ref="SimpleAddress" />
            <xs:element ref="StructuredAddress" />
            <xs:element ref="PaymentAddress" />
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attributeGroup ref="ObjAssociationAttrGroup" />
  </xs:complexType>
  <xs:complexType name="DirectBillMethodType">
    <xs:annotation>
