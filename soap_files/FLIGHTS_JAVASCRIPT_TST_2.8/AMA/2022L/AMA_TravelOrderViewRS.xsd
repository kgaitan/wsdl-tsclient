          <xs:documentation source="description" xml:lang="en">Purchaser bank account number. Example: 067014822</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="ContextSimpleType">
              <xs:attributeGroup ref="ObjAssociationAttrGroup" />
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
