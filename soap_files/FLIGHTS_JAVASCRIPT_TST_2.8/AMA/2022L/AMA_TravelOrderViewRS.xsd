    <xs:annotation>
      <xs:documentation xml:lang="en">PAYMENT CARD Payment Method definition.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CardType" type="PayCardTypeSimpleType" minOccurs="0">
        <xs:annotation>
