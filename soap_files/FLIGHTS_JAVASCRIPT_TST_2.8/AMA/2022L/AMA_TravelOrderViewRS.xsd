                <xs:annotation>
                  <xs:documentation source="description" xml:lang="en">When true, Simple Pricing  for flight offers is requested.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
